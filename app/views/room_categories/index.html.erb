<h1>Listing room_categories</h1>

<table>
  <thead>
    <tr>
      <th>Room</th>
      <th>Category</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @room_categories.each do |room_category| %>
      <tr>
        <td><%= room_category.room.name %></td>
        <td><%= room_category.category.name %></td>
        <td><%= link_to 'Show', room_category %></td>
        <td><%= link_to 'Edit', edit_room_category_path(room_category) %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>